//Write a C++ program for setfill function.

#include <iostream>
#include <iomanip> 
using namespace std;
int main() 
{
    int number = 123;

    cout<<"My Name Is Vadariya Shrey"<<endl;
    cout<<"En_no :: 220130318054 "<<endl;
    cout << "The value is : " << setfill('#') << setw(10) << number << endl;
    
    return 0;
}
