//Write a C++ to demonstrate use of return by refrence.

#include<iostream>
using namespace std;
int x = 5;      //global declaration
int &test();
int main()
{
    cout<<"My Name Is Vadariya Shrey"<<endl;
    cout<<"En_no :: 220130318054 "<<endl;
    test()=15;
    cout<<test();
    return 0;
}
int &test()
{
    return x;
}
